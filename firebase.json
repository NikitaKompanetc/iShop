{
  "functions": {

    "predeploy": [
      "npm --prefix \"functions\" run install",
      "npm --prefix \"functions\" run build"
    ]
  },
  "hosting": {
    "public": "dist/angularFirebase",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
